  <h:head>
    <h:title>user.test.1</h:title>
    <model>
      <instance>
        <data id="build_user-test-1_1570740835">
          <meta>
            <instanceID/>
          </meta>
          <username/>
          <password/>
        </data>
      </instance>
      <itext>
        <translation lang="English">
          <text id="/data/username:label">
            <value>Username</value>
          </text>
          <text id="/data/password:label">
            <value>Password</value>
          </text>
        </translation>
      </itext>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
      <bind nodeset="/data/username" type="string"/>
      <bind nodeset="/data/password" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/username">
      <label ref="jr:itext('/data/username:label')"/>
    </input>
    <input ref="/data/password">
      <label ref="jr:itext('/data/password:label')"/>
    </input>
  </h:body>
